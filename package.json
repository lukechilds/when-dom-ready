{
  "name": "when-dom-ready",
  "version": "1.2.5",
  "description": "$(document).ready() for the 21st century",
  "main": "dist/es5.umd/index.js",
  "module": "src/index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "pretest": "npm run build",
    "lint": "xo",
    "test": "nyc ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run build"
  },
  "ava": {
    "require": [
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "xo": {
    "env": "browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukechilds/when-dom-ready.git"
  },
  "keywords": [
    "check",
    "dom",
    "loaded",
    "ready",
    "promise",
    "async",
    "asynchronous",
    "pure"
  ],
  "author": "Luke Childs <lukechilds123@gmail.com> (http://lukechilds.co.uk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukechilds/when-dom-ready/issues"
  },
  "homepage": "https://github.com/lukechilds/when-dom-ready",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.17.0",
    "browser-env": "^2.0.19",
    "coveralls": "^2.11.15",
    "jsdom": "^9.9.1",
    "nyc": "^10.0.0",
    "typescript": "^2.1.5",
    "xo": "^0.17.1"
  }
}
